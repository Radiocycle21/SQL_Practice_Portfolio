-- induvidual checks

select * from  customers; 
select count(*) from customers ;

select * from department ; 
select count(*) from department ;

select * from customer_actions ; 
select count(*) from customer_actions ;

truncate table customer_actions;

select * from employees ; 
select count(*) from employees ;

select * from orders ; 
select count(*) from orders;

select * from order_details; 
select count(*) from order_details;

select * from products ; 
select count(*) from products ;

select * from sales ; 
select count(*) from sales;


-- ----------------------------------------------------------------------------------------------------
    -- Report -- 
-- ----------------------------------------------------------------------------------------------------

-- Get the count of rows for each table to verify data insertion. 
-- select all the lines below and run.
-- ----------------------------------------------------------------------------------------------------

-- Count of rows in the department table
SELECT 
'department' AS TableName, 
COUNT(*) as Number_rows
FROM department

UNION ALL

-- Count of rows in the employees table
SELECT 'employees', 
COUNT(*)
FROM employees

UNION ALL

-- Count of rows in the customers table
SELECT 'customers', 
COUNT(*) 
FROM customers

UNION ALL

-- Count of rows in the products table
SELECT 'products', 
COUNT(*) 
FROM products

UNION ALL

-- Count of rows in the orders table
SELECT 'orders', 
COUNT(*) 
FROM orders

UNION ALL

-- Count of rows in the order_details table
SELECT 'order_details', 
COUNT(*)
FROM order_details

UNION ALL

-- Count of rows in the sales table
SELECT 'sales', 
COUNT(*) 
FROM sales

UNION ALL

-- Count of rows in the customer_actions table
SELECT 'customer_actions', 
COUNT(*) 
FROM customer_actions;
